{
  "modules": [
    {
      "name": "CSVOutSink",
      "package": "cloudbrain.modules.sinks.csvout",
      "options": {
        "out_dir": "/tmp/cloudbrain"
      },
      "subscribers": [
        {
          "name": "PikaSubscriber",
          "package": "cloudbrain.subscribers.rabbitmq",
          "options": {
            "rabbitmq_address": "localhost",
            "rabbitmq_user": "guest",
            "rabbitmq_pwd": "guest",
	    "rabbitmq_vhost": "/"
          },
          "base_routing_key": "some_unique_id:device_name",
          "metrics": [
            {
              "metric_name": "eeg",
              "num_channels": 8
            }
          ]
        }
      ],
      "publishers": []
    }
  ]
}
