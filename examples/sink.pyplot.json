{
  "modules": [
    {
      "name": "PyPlotSink",
      "package": "cloudbrain.modules.sinks.pyplot",
      "options": {
        "channels_to_plot": [0],
        "autoscale": true,
        "y_min": -15,
        "y_max": 15
      },
      "subscribers": [
        {
          "name": "PikaSubscriber",
          "package": "cloudbrain.subscribers.rabbitmq",
          "options": {
            "rabbitmq_address": "localhost",
            "rabbitmq_user": "guest",
            "rabbitmq_pwd": "guest",
            "rabbitmq_vhost": "/"
          },
          "base_routing_key": "some_unique_key",
          "metrics": [
            {
              "metric_name": "eeg",
              "num_channels": 8
            }
          ]
        }
      ],
      "publishers": []
    }
  ]
}
